<div class="container">
  <%= simple_form_for [@flat, @booking] do |f| %>
  <%= f.input :start_date, as: :datetime, html5: true, order: %i(day month year), label: "From" %>
  <%= f.input :end_date, as: :datetime, html5: true, order: %i(day month year), label: "To" %>
  <%= f.submit "Create booking", class: 'btn btn-primary' %>
  <% end %>
</div>
